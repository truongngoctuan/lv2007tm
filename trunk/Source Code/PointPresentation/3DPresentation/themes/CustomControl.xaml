                   Height="8"
                   HorizontalAlignment="Center"
                   Margin="0,-1,0,0"
                   VerticalAlignment="Center"
                   Width="9"
                   Opacity="1"
                   RenderTransformOrigin="0.5,0.5"
                   Visibility="Collapsed"
                   Data="F1 M 6.742676,3.852539 L 9.110840,1.559570 L 8.910645,0.500000 L 6.838379,0.500000 L 4.902832,2.435547 L 2.967285,0.500000 L 0.895020,0.500000 L 0.694824,1.559570 L 3.062988,3.852539 L 0.527832,6.351563 L 0.689941,7.600586 L 2.967285,7.600586 L 4.897949,5.575195 L 6.854004,7.600586 L 9.115723,7.600586 L 9.277832,6.351563 L 6.742676,3.852539 Z">
                                                    <Path.RenderTransform>
                                                        <TransformGroup>
                                                            <ScaleTransform ScaleX="1.3"
                                ScaleY="1.3" />
                                                        </TransformGroup>
                                                    </Path.RenderTransform>
                                                </Path>
                                                <!--Styling the CloseButtons cross edges Fill for MouseOver: New Color-->
                                                <Path x:Name="X_Fuzz1"
                   Fill="#FFC9756E"
                   Stretch="Fill"
                   Stroke="#1EC51900"
                   Height="8"
                   HorizontalAlignment="Center"
                   Margin="0,-1,0,0"
                   VerticalAlignment="Center"
                   Width="9"
                   Opacity="1"
                   RenderTransformOrigin="0.5,0.5"
                   Visibility="Collapsed"
                   Data="F1 M 6.742676,3.852539 L 9.110840,1.559570 L 8.910645,0.500000 L 6.838379,0.500000 L 4.902832,2.435547 L 2.967285,0.500000 L 0.895020,0.500000 L 0.694824,1.559570 L 3.062988,3.852539 L 0.527832,6.351563 L 0.689941,7.600586 L 2.967285,7.600586 L 4.897949,5.575195 L 6.854004,7.600586 L 9.115723,7.600586 L 9.277832,6.351563 L 6.742676,3.852539 Z">
                                                    <Path.RenderTransform>
                                                        <TransformGroup>
                                                            <ScaleTransform ScaleX="1.1"
                                ScaleY="1.1" />
                                                        </TransformGroup>
                                                    </Path.RenderTransform>
                                                </Path>
                                                <!--Styling the CloseButtons cross edges Fill for MouseOver: New Color-->
                                                <Path x:Name="X_Fuzz0"
                   Fill="#FFC9756E"
                   Stretch="Fill"
                   Stroke="#FFC51900"
                   Height="8"
                   HorizontalAlignment="Center"
                   Margin="0,-1,0,0"
                   VerticalAlignment="Center"
                   Width="9"
                   Opacity="1"
                   Visibility="Collapsed"
                   Data="F1 M 6.742676,3.852539 L 9.110840,1.559570 L 8.910645,0.500000 L 6.838379,0.500000 L 4.902832,2.435547 L 2.967285,0.500000 L 0.895020,0.500000 L 0.694824,1.559570 L 3.062988,3.852539 L 0.527832,6.351563 L 0.689941,7.600586 L 2.967285,7.600586 L 4.897949,5.575195 L 6.854004,7.600586 L 9.115723,7.600586 L 9.277832,6.351563 L 6.742676,3.852539 Z" />
                                                <!--Styling the CloseButtons cross Fill: new Color-->
                                                <Path x:Name="X"
                   Fill="#FFFF8453"
                   Stretch="Fill"
                   Height="8"
                   HorizontalAlignment="Center"
                   Margin="0,-1,0,0"
                   VerticalAlignment="Center"
                   Width="9"
                   Opacity="0.7"
                   Data="F1 M 6.742676,3.852539 L 9.110840,1.559570 L 8.910645,0.500000 L 6.838379,0.500000 L 4.902832,2.435547 L 2.967285,0.500000 L 0.895020,0.500000 L 0.694824,1.559570 L 3.062988,3.852539 L 0.527832,6.351563 L 0.689941,7.600586 L 2.967285,7.600586 L 4.897949,5.575195 L 6.854004,7.600586 L 9.115723,7.600586 L 9.277832,6.351563 L 6.742676,3.852539 Z">
                                                    <Path.Stroke>
                                                        <LinearGradientBrush EndPoint="0.5,1"
