<UserControl x:Class="_3DPresentation.TourControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:my="clr-namespace:_3DPresentation"  
    mc:Ignorable="d"
    xmlns:Babylon="clr-namespace:Babylon;assembly=Babylon"  
    d:DesignHeight="300" d:DesignWidth="400">
    
    <Grid x:Name="LayoutRoot" Background="Transparent">
        <Babylon:BabylonSurface x:Name="babylonSurface" Background="Transparent"/>        
    </Grid>
</UserControl>
