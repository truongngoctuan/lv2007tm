<UserControl x:Class="_3DPresentation.Views.Catalog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="768" d:DesignWidth="1024">
    
    <Grid x:Name="LayoutRoot" Background="White">
        <Image Source="Images/editor_view_bg.jpg" Stretch="Fill"></Image>
        
        <Button Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" x:Name="btBack" Style="{StaticResource ButtonStyleYellow}" Content="Back" Width="75" Height="23" Margin="20, 20,20, 20"/>
        <Button Foreground="White" VerticalAlignment="Top" HorizontalAlignment="Left" x:Name="btNewTour" Style="{StaticResource ButtonStyleYellow}" Content="New Tour" Width="75" Height="23" Margin="20, 70, 20, 20"/>
        
        <StackPanel x:Name="stackPanel" HorizontalAlignment="Center" VerticalAlignment="Center">            
        </StackPanel>
    </Grid>
</UserControl>
